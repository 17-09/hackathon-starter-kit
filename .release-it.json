{
    "git": {
      "tagName": "v${version}"
    },
    "github": {
      "release": true,
      "releaseNotes": "git log --pretty=format:'* %s (%h)' [REV_RANGE]"
    },
    "plugins": {
        "@release-it/conventional-changelog": {
          "preset": "angular",
          "infile": "CHANGELOG.md"
        }
      }
  }

